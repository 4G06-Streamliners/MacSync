{
  "name": "@teamd/capstonemock",
  "version": "0.1.0",
  "description": "Team D CapstoneMock project",
  "private": true,
  "workspaces": [
    "src/backend",
    "src/frontend/web-admin",
    "src/frontend/mobile"
  ],
  "packageManager": "pnpm@10.18.0",
  "pnpm": {
    "overrides": {
      "react": "18.3.1",
      "react-dom": "18.3.1"
    }
  },
  "scripts": {
    "dev": "concurrently \"pnpm --filter backend dev\" \"pnpm --filter web-admin dev\"",
    "dev:backend": "pnpm --filter backend dev",
    "dev:admin": "pnpm --filter web-admin dev",
    "dev:mobile": "pnpm --filter mobile start",
    "build": "pnpm -r build",
    "build:backend": "pnpm --filter backend build",
    "build:admin": "pnpm --filter web-admin build",
    "build:mobile": "pnpm --filter mobile build",
    "lint": "pnpm -r run lint",
    "clean": "pnpm -r run clean"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.74.5"
  },
  "devDependencies": {
    "@types/react": "~18.2.79",
    "concurrently": "^8.2.2"
  }
}
